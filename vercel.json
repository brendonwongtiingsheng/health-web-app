{
  "rewrites": [
    { "source": "/remoteEntry.js", "destination": "/remoteEntry.js" },

    { "source": "/(.*\\.js)", "destination": "/$1" },
    { "source": "/(.*\\.css)", "destination": "/$1" },
    { "source": "/(.*\\.map)", "destination": "/$1" },
    { "source": "/(.*\\.(png|jpg|jpeg|gif|svg|webp|ico))", "destination": "/$1" },

    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
